{
  "name": "nuxt-app",
  "version": "1.0.1",
  "private": false,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "start": "nuxt generate && nuxt start",
    "go": "nuxt build && yarn dev",
    "refresh": "rm -rf node_modules/ .nuxt/ dist/ && yarn install && yarn go",
    "analyze": "nuxt build --analyze",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{s?(a|c)ss,css,vue}\" --ignore-path .gitignore",
    "prettier": "prettier ./**/*.{js,vue} --ignore-path .gitignore",
    "check:prettier": "yarn prettier --list-different",
    "format:prettier": "yarn prettier --write --loglevel warn",
    "lint": "yarn lint:js && yarn lint:style"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@mdi/js": "^5.9.55",
    "@nuxt/components": "^2.1.3",
    "@nuxt/content": "^1.11.1",
    "@nuxtjs/axios": "^5.13.1",
    "@nuxtjs/gtm": "^2.4.0",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/robots": "^2.5.0",
    "@nuxtjs/sitemap": "^2.4.0",
    "@nuxtjs/yandex-metrika": "^1.3.0",
    "animated-number-vue": "^1.0.0",
    "core-js": "^3.9.1",
    "markdown-it-attrs": "^4.0.0",
    "markdown-it-div": "^1.1.0",
    "nuxt": "^2.15.3",
    "nuxt-i18n": "^6.21.1",
    "vue-cookieconsent-component": "^1.2.0",
    "vue-disqus": "^4.0.1",
    "vue-if-bot": "^1.2.0",
    "vue-notification": "^1.3.20",
    "vue2-perfect-scrollbar": "^1.5.0",
    "vuedraggable": "^2.24.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@fullhuman/postcss-purgecss": "^4.0.2",
    "@nuxtjs/eslint-config": "^5.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/google-analytics": "^2.4.0",
    "@nuxtjs/google-fonts": "^1.2.0",
    "@nuxtjs/html-validator": "^0.2.1",
    "@nuxtjs/markdownit": "^2.0.0",
    "@nuxtjs/style-resources": "^1.0.0",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@nuxtjs/svg": "^0.1.12",
    "@nuxtjs/vuetify": "^1.11.3",
    "apexcharts": "^3.26.0",
    "babel-eslint": "^10.1.0",
    "css-byebye": "^2.0.2",
    "css-loader": "^5.1.1",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.6.0",
    "husky": "^5.1.3",
    "lint-staged": "^10.5.4",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "nuxt-purgecss": "^1.0.0",
    "postcss": "^8.2.10",
    "postcss-custom-media": "^8.0.0",
    "postcss-hexrgba": "^2.0.1",
    "postcss-import": "^13.0.0",
    "postcss-loader": "^4.1.0",
    "postcss-mixins": "^7.0.3",
    "postcss-nested": "^5.0.3",
    "postcss-responsive-type": "^1.0.0",
    "postcss-scss": "^3.0.4",
    "postcss-sorting": "^6.0.0",
    "postcss-url": "^10.1.1",
    "postcss-utilities": "^0.8.4",
    "precss": "^4.0.0",
    "prettier": "^2.2.1",
    "prism-themes": "^1.5.0",
    "pug": "^3.0.2",
    "pug-plain-loader": "^1.1.0",
    "stylelint": "^13.11.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^21.0.0",
    "stylus": "^0.54.8",
    "stylus-loader": "4.3.1",
    "v-clipboard": "^2.2.3",
    "vue-apexcharts": "^1.6.1",
    "vue-github-button": "^1.3.0"
  },
  "main": "nuxt.config.js",
  "repository": "https://github.com/andrejsharapov/nuxt-app.git",
  "author": "andrejsharapov",
  "license": "MIT"
}
