{
  "automerge": false,
  "baseBranches": [
    "master"
  ],
  "extends": [
    "config:base",
    ":semanticCommits",
    ":semanticCommitTypeAll(chore)"
  ],
  "ignoreDeps": [
    "@nuxt/content",
    "@nuxtjs/google-fonts",
    "@nuxtjs/html-validator",
    "nuxt-purgecss",
    "css-loader",
    "postcss",
    "postcss-custom-media",
    "postcss-hexrgba",
    "postcss-html",
    "postcss-mixins",
    "postcss-nested",
    "postcss-sorting",
    "postcss-url",
    "prism-themes",
    "stylelint",
    "stylus",
    "stylus-loader",
    "vue-github-button"
  ],
  "ignorePaths": [
    "**/tests/**"
  ],
  "labels": [
    "Dependencies"
  ],
  "lockFileMaintenance": {
    "enabled": true
  },
  "major": {
    "stabilityDays": 14
  },
  "minor": {
    "stabilityDays": 7
  },
  "packageRules": [
    {
      "matchDepTypes": [
        "devDependencies"
      ],
      "automerge": true
    }
  ]
}